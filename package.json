{
  "name": "ts-lazychecker",
  "version": "0.0.1",
  "description": "TS-Core is a github repo template for TypeScript Projects",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "ts-estest ./src",
    "prepublishOnly": "npm run build",
    "build": "tsc -p ./tsconfig.build.json",
    "start:debug": "node --loader @k-foss/ts-esnode --experimental-modules --experimental-specifier-resolution=node --harmony-top-level-await ./",
    "dev": "nodemon",
    "prettier": "prettier --config .prettierrc --check \"src/**/*ts\"",
    "lint": "eslint ./src --ext .js,.ts",
    "listMachines": "node --loader @k-foss/ts-esnode --experimental-modules --experimental-specifier-resolution=node --harmony-top-level-await ./src/bin/listMachines.ts",
    "run:generateSchema": "node --loader @k-foss/ts-esnode --experimental-modules --experimental-specifier-resolution=node --harmony-top-level-await ./src/bin/generateSchema.ts"
  },
  "dependencies": {
    "@elastic.io/ntlm-client": "^1.0.0",
    "@k-foss/ts-esnode": "^1.7.0",
    "adaptivecards": "^2.6.0",
    "ajv": "^7.0.3",
    "bullmq": "^1.14.0",
    "cron-parser": "^3.1.0",
    "date-fns": "^2.16.1",
    "fluent-json-schema": "^2.0.3",
    "got": "^11.8.1",
    "hyperid": "^2.1.0",
    "js-yaml": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@eclass/semantic-release-docker": "^1.0.6",
    "@k-foss/ts-estests": "^1.0.6",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.21",
    "@types/ntlm-client": "^0.1.0",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "fastify": "^3.10.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.3"
  },
  "author": "",
  "license": "MIT"
}
